{
  "name": "todo-server",
  "version": "1.0.0",
  "description": "Todo app API for portfolio project",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "postinstall": "npm run migrate:up",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon index.js",
    "migrate:up": "if [ -e .env ]; then . ./.env; fi && sequelize db:migrate --url $DATABASE_URL",
    "migrate:down": "if [ -e .env ]; then . ./.env; fi && sequelize db:migrate:undo --url $DATABASE_URL"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^3.0.8",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.18.2",
    "express-promise-router": "^3.0.3",
    "jsonwebtoken": "^8.5.1",
    "nodemon": "^2.0.2",
    "pg": "^7.18.1",
    "sequelize": "^5.21.3",
    "sequelize-cli": "^5.5.1",
    "swagger-node-express": "^2.1.3",
    "swagger-ui-express": "^4.1.3",
    "yamljs": "^0.3.0"
  }
}
